{
  "name": "em-icp-api",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "start": "NODE_ENV=production ./node_modules/.bin/ts-node -r tsconfig-paths/register server.ts",
    "start:dev": "nodemon --config nodemon.json",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "NODE_ENV=test yarn test:unit",
    "test:unit": "mocha --require ts-node/register 'test/unit/**/*.test.ts' --reporter spec --recursive --exit",
    "test:coverage": "cross-env NODE_ENV=test NODE_PATH=. nyc yarn test:unit",
    "test:a11y": "cross-env NODE_PATH=.",
    "sonar-scanner": "node_modules/sonar-scanner/bin/sonar-scanner",
    "sonar-scan": "cross-env NODE_PATH=. sonar-scanner",
    "test:smoke": "NODE_ENV=test mocha -r ts-node/register 'test/smoke/healthcheck.test.ts' --timeout 25000 --reporter spec --exit",
    "test:functional": "cross-env NODE_PATH=. mocha --require ts-node/register 'test/functional/*.test.ts' --timeout 25000 --reporter spec --recursive --exit",
    "test:fullfunctional": "yarn test:functional",
    "cichecks": "yarn && yarn lint  && yarn test"
  },
  "dependencies": {
    "@hmcts/info-provider": "1.0.0",
    "@hmcts/nodejs-healthcheck": "1.7.0",
    "@hmcts/nodejs-logging": "3.0.4",
    "@hmcts/properties-volume": "0.0.13",
    "@types/config": "0.0.38",
    "@types/cookie-parser": "1.4.2",
    "@types/csurf": "1.11.2",
    "@types/es6-promisify": "6.0.1",
    "@types/express": "4.17.13",
    "@types/helmet": "4.0.0",
    "@types/node": "14.18.12",
    "@types/require-directory": "2.1.2",
    "@types/uuid": "8.3.4",
    "ansi-regex": "6.0.1",
    "applicationinsights": "1.8.10",
    "axios": "0.21.4",
    "chai-http": "4.3.0",
    "config": "3.3.7",
    "cookie-parser": "1.4.6",
    "csurf": "1.11.0",
    "engine.io": "6.1.3",
    "express": "4.17.3",
    "express-rate-limit": "5.5.1",
    "follow-redirects": "1.14.9",
    "helmet": "3.23.3",
    "ioredis": "4.28.5",
    "ioredis-mock": "4.21.8",
    "jsonwebtoken": "8.5.1",
    "jwk-to-pem": "2.0.5",
    "jwt-decode": "3.1.2",
    "lodash": "4.17.21",
    "nanoid": "3.3.2",
    "require-directory": "2.1.1",
    "socket.io": "3.1.1",
    "socket.io-client": "^3.1.1",
    "superagent": "6.1.0",
    "swagger-ui-dist": "4.10.3",
    "swagger-ui-express": "4.3.0",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.14.1",
    "typescript": "4.6.3",
    "typescript-eslint": "0.0.1-alpha.0",
    "uuid": "3.4.0",
    "ws": "7.5.7",
    "xmlhttprequest-ssl": "1.6.3"
  },
  "resolutions": {
    "xmlhttprequest-ssl": "1.6.3",
    "lodash": "^4.17.21",
    "y18n": "^5.0.5",
    "ws": "7.5.7",
    "axios": "0.21.4"
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@babel/preset-env": "7.16.11",
    "@types/chai": "4.3.0",
    "@types/chai-as-promised": "7.1.5",
    "@types/mocha": "8.2.3",
    "@types/sinon": "7.5.2",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/eslint-plugin-tslint": "2.34.0",
    "@typescript-eslint/parser": "3.10.1",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.4",
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "cross-env": "7.0.3",
    "debug": "4.3.4",
    "eslint": "7.32.0",
    "mocha": "9.2.2",
    "nock": "11.9.1",
    "nodemon": "2.0.15",
    "nyc": "15.1.0",
    "pa11y": "5.3.1",
    "request": "2.88.2",
    "sinon": "9.2.4",
    "sinon-chai": "3.7.0",
    "sonar-scanner": "3.1.0",
    "ts-loader": "6.2.2",
    "tslint": "5.20.1"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "api/**/*.ts"
    ],
    "exclude": [
      "test/**/*.ts"
    ],
    "reporter": "lcov"
  }
}
