    "tslint": "5.20.1"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "api/**/*.ts"
    ],
    "exclude": [
      "test/**/*.ts"
    ],
    "reporter": "lcov"
  },
  "packageManager": "yarn@3.5.1"
}
